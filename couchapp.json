{
    "name": "Sharebill",
    "description": "An application to help you share bills",
    "hooks": {
      "pre-push": [
        ["py", "hooks/configjs.py"],
        ["py", "couchapp.hooks.compress"]
      ]
    },
    "compress": {
      "js": {
        "vendor/couchapp/lol.js": [
          "vendor/couchapp/_attachments/sha1.js",
          "vendor/couchapp/_attachments/json2.js",
          "vendor/couchapp/_attachments/jquery.js",
          "vendor/couchapp/_attachments/jquery.couch.js",

          "vendor/couchapp/_attachments/jquery.couch.app.js",
          "vendor/couchapp/_attachments/jquery.couch.app.util.js",
          "vendor/couchapp/_attachments/jquery.mustache.js",
          "vendor/couchapp/_attachments/jquery.evently.js"
        ]
      }
    }
}