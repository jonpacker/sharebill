[
	{
		"from": "",
		"to": "index.html",
		"method": "GET"
	},


	"Smart rewrites",
	{
		"from": "post/:pid",
		"to": "_show/freeform/:pid",
		"method": "GET"
	}, {
		"from": "post/:pid",
		"to": "../../:pid",
		"method": "PUT"
	}, {
		"from": "post/:pid",
		"to": "../../:pid",
		"method": "DELETE"
	}, {
		"from": "account/:uid",
		"to": "_list/account_page/user",
		"query": {
			"uid": ":uid",
			"startkey": [ ":uid", {} ],
			"endkey": [ ":uid" ],
			"descending": "true",
			"limit": "10"
		}
	}, {
		"from": "account/:uid/posts",
		"to": "_list/account_posts/user",
		"query": {
			"startkey": [ ":uid", {} ],
			"endkey": [ ":uid" ],
			"descending": "true",
			"limit": "10"
		}
	}, {
		"from": "account/:uid/debit",
		"to": "_list/balance/totals",
		"query": {
			"group": "true",
			"startkey": [ ":uid", "debets" ],
			"endkey": [ ":uid", "debets", {} ]
		}
	}, {
		"from": "account/:uid/credit",
		"to": "_list/balance/totals",
		"query": {
			"group": "true",
			"startkey": [ ":uid", "credits" ],
			"endkey": [ ":uid", "credits", {} ]
		}
	}, {
		"from": "account/:uid/balance",
		"to": "_list/balance/totals",
		"query": {
			"group": "true",
			"startkey": [ ":uid", "credits" ],
			"endkey": [ ":uid", "debets" ]
		}
	}, {
		"from": "account/:uid/totals",
		"to": "_view/totals",
		"query": {
			"reduce": "true",
			"group": "true",
			"startkey": [ ":uid" ],
			"endkey": [ ":uid", {} ]
		}
	}, {
		"from": "balances",
		"to": "_view/balances",
		"query": { "group": "true", "reduce": "true" }
	}, {
		"from": "recent",
		"to": "_view/all",
		"query": { "limit": "10", "descending": "true" }
	}, {
		"from": "instance_config",
		"to": "../../instance_config"
	},


	"Basic fallback rewrites",
	{
		"to": "vendor/*",
		"from": "vendor/*"
	}, {
		"to": "app/*",
		"from": "app/*"
	}, {
		"to": "config/*",
		"from": "config/*"
	}, {
		"to": "_view/*",
		"from": "_view/*"
	}, {
		"to": "",
		"from": "the_database/_design/the_design_document"
	}, {
		"to": "*",
		"from": "the_database/_design/the_design_document/*"
	}, {
		"to": "*",
		"from": "the_design_document/*"
	}, {
		"to": "../../",
		"from": "the_database"
	}, {
		"to": "../../*",
		"from": "the_database/*"
	}, {
		"to": "*",
		"from": "*"
	}
]
